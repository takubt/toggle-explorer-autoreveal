{
    "name": "toggleexplorerautoreveal",
    "displayName": "Toggle explorer.autoReveal",
    "description": "",
    "version": "0.0.4",
    "publisher": "takubt",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/takubt/toggle-explorer-autoreveal.git",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/takubt/toggle-explorer-autoreveal/issues"
    },
    "homepage": "https://github.com/takubt/toggle-explorer-autoreveal/blob/develop/README.md",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "toggle",
        "explorer",
        "autoreveal"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "toggleexplorerautoreveal.toggle",
                "title": "autoReveal toggle"
            },
            {
                "command": "toggleexplorerautoreveal.on",
                "title": "autoReveal on"
            },
            {
                "command": "toggleexplorerautoreveal.off",
                "title": "autoReveal off"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "toggleexplorerautoreveal.toggle",
                    "title": "Toggle autoReveal",
                    "group": "toggleexplorerautoreveal"
                },
                {
                    "command": "toggleexplorerautoreveal.on",
                    "title": "autoReveal on",
                    "group": "toggleexplorerautoreveal"
                },
                {
                    "command": "toggleexplorerautoreveal.off",
                    "title": "autoReveal off",
                    "group": "toggleexplorerautoreveal"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.34"
    }
}
